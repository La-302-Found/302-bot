{
  "name": "302-bot",
  "version": "1.0.0",
  "description": "The discord bot for the 302 server",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tspc",
    "start": "node dist/index.js",
    "format": "prettier --write .",
    "typecheck": "tspc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint src/**/*.ts",
    "lint:prettier": "prettier --check .",
    "db:up": "docker compose -f docker/db.compose.yml up -d",
    "db:down": "docker compose -f docker/db.compose.yml down",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate && prettier --write migrations",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push"
  },
  "keywords": [
    "discord",
    "302",
    "bot"
  ],
  "author": "Kan-A-Pesh",
  "private": true,
  "dependencies": {
    "@openrouter/ai-sdk-provider": "^0.7.1",
    "@types/winston": "^2.4.4",
    "ai": "^4.3.16",
    "cron": "^4.1.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "luxon": "^3.5.0",
    "pg": "^8.16.2",
    "redis": "^4.7.0",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/luxon": "^3.4.2",
    "@types/pg": "^8.15.4",
    "@types/redis": "^4.0.10",
    "drizzle-kit": "^0.31.1",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "ts-patch": "^3.3.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "typescript-transform-paths": "^3.5.3"
  }
}
